{
  "name": "@kcp/tags",
  "version": "1.0.0",
  "description": "A Klasa plugin to add tag functionality to bots",
  "homepage": "https://github.com/KlasaCommunityPlugins/tags",
  "bugs": {
    "url": "https://github.com/KlasaCommunityPlugins/tags/issues"
  },
  "author": "bdistin",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KlasaCommunityPlugins/tags.git"
  },
  "scripts": {
    "test:lint": "yarn tslint 'src/*.ts' && yarn eslint --config .eslintrcmd.json --ext md guides && yarn markdownlint guides README.md",
    "build": "yarn tsc",
    "docs": "yarn build && yarn dg --source dist --custom guides/.docconfig.json --output dist/docs.json --logging"
  },
  "peerDependencies": {
    "discord.js": "github:discordjs/discord.js#master",
    "klasa": "github:dirigeants/klasa#master"
  },
  "devDependencies": {
    "@kcp/lint": "github:KlasaCommunityPlugins/lint-files",
    "@types/node": "^14.0.1",
    "@types/ws": "^7.2.0",
    "discord.js": "github:discordjs/discord.js#master",
    "docgen": "github:dirigeants/docsgen",
    "eslint": "^7.0.0",
    "eslint-config-klasa": "github:dirigeants/klasa-lint",
    "eslint-plugin-markdown": "^2.1.0",
    "klasa": "github:dirigeants/klasa#master",
    "markdownlint-cli": "^0.15.0",
    "tslint": "^6.0.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}
